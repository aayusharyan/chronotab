{
  "name": "TokiTab",
  "description": "TokiTab turns your new tab into a gallery of animated clock faces. Pick a style and stay on time.",
  "version": "0.1.16",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  },
  "icons": {
    "512": "src/assets/img/icon-512.png",
    "256": "src/assets/img/icon-256.png",
    "128": "src/assets/img/icon-128.png",
    "48": "src/assets/img/icon-48.png",
    "16": "src/assets/img/icon-16.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; style-src 'self' 'unsafe-inline'; object-src 'self'"
  }
}
